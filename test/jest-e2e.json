{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "./../",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "globalSetup": "./test/utils/setup.ts",
  "collectCoverageFrom": [
    "src/**/*.ts"
  ],
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    ".spec.ts$",
    "<rootDir>/src/main.ts$"
  ],
  "coverageDirectory": "./test/coverage"
}
